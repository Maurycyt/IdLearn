            </Text>
            <Text layoutX="170.0" layoutY="349.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="creditsHeaderText" text="Resources used">
            </Text>
            <Text fx:id="resourcesText" layoutX="371.0" layoutY="284.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sample Text" />
         </children></VBox>
    </center>
    <stylesheets>
        <URL value="@../style.css" />
        <URL value="@../common_style.css" />
