<BorderPane fx:id="mainBorderPane" maxHeight="-Infinity" maxWidth="-Infinity" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mimuw.idlearn.scenes.controllers.CreditsController">
    <center>
        <VBox fx:id="menuVBox" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" style="-fx-spacing: 20px;" styleClass="menuVBox" BorderPane.alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="creditsHeaderText" text="This game was made by">
            </Text>
            <Text fx:id="authorsText" strokeType="OUTSIDE" strokeWidth="0.0" text="Sample Text" />
